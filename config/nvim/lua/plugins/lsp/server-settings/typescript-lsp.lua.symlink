-- vi: ft=lua

local M = {}

function M.setup()
  local typescript_status_ok, typescript = pcall(require, "typescript")
  if not typescript_status_ok then
    return
  end

  typescript.setup({
    debug = false, -- enable debug logging for commands
    server = {
      on_attach = require("plugins.lsp.on_attach"),
    },
  })
end

return M
