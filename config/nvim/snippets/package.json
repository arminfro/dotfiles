{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./javascript/react-ts.json"
      },
      {
        "language": [
          "cs"
        ],
        "path": "./cs.json"
      },
      {
        "language": [
          "lua"
        ],
        "path": "./lua.json"
      },
      {
        "language": [
          "rust"
        ],
        "path": "./rust.json"
      }
    ]
  }
}
